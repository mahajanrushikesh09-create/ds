customer_ids = [101, 104, 106, 109, 115, 120, 130, 145, 150]
def linear_search(ids, key):
    for i in range(len(ids)):
        if ids[i] == key:
            return True
    return False
def binary_search(ids, key):
    low = 0
    high = len(ids) - 1
    while low <= high:
        mid = (low + high) // 2
        if ids[mid] == key:
            return True
        elif ids[mid] < key:
            low = mid + 1
        else:
            high = mid - 1
    return False
search_id = int(input("Enter Customer Account ID to search: "))
if linear_search(customer_ids, search_id):
    print("Linear Search: Customer ID FOUND ✅")
else:
    print("Linear Search: Customer ID NOT FOUND ❌")
if binary_search(customer_ids, search_id):
    print("Binary Search: Customer ID FOUND ✅")
else:
    print("Binary Search: Customer ID NOT FOUND ❌")
